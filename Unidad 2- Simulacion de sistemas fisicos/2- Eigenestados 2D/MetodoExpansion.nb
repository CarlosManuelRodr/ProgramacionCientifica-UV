           RowBox[{"basisIndexes", "[", 
            RowBox[{"[", 
             RowBox[{"k1", ",", "1"}], "]"}], "]"}], ",", 
           RowBox[{"basisIndexes", "[", 
            RowBox[{"[", 
             RowBox[{"k1", ",", "2"}], "]"}], "]"}]}], "]"}], "*", 
         RowBox[{"FrII", "[", 
          RowBox[{"x", ",", "y"}], "]"}], "*", 
         RowBox[{"\[Phi]m", "[", 
          RowBox[{"x", ",", "y", ",", 
           RowBox[{"basisIndexes", "[", 
            RowBox[{"[", 
             RowBox[{"k2", ",", "1"}], "]"}], "]"}], ",", 
           RowBox[{"basisIndexes", "[", 
            RowBox[{"[", 
             RowBox[{"k2", ",", "2"}], "]"}], "]"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{"x", ",", "0", ",", "2"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"y", ",", "0", ",", "2"}], "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{"AccuracyGoal", "\[Rule]", "8"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Method", "\[Rule]", "\"\<MultidimensionalRule\>\""}]}], 
       "\[IndentingNewLine]", "]"}]}], ",", "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"k1", ",", "1", ",", 
       RowBox[{"Length", "[", "basisIndexes", "]"}]}], "}"}], ",", 
