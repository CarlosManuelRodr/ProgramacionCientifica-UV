Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"FrII", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"x", "-", "1"}], ")"}], "2"], "+", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"y", "-", "1"}], ")"}], "2"]}], "<", "1"}], "&&", 
      RowBox[{"x", "<", 
       RowBox[{"1", "+", "cu"}]}]}], ",", "0", ",", "1"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[Phi]m", "[", 
    RowBox[{"x_", ",", "y_", ",", "m1_", ",", "m2_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Sin", "[", 
     FractionBox[
      RowBox[{"\[Pi]", " ", "m1", " ", "x"}], "2"], "]"}], 
    RowBox[{"Sin", "[", 
     FractionBox[
      RowBox[{"\[Pi]", " ", "m2", " ", "y"}], "2"], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"basisIndexes", " ", "=", " ", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"m1", ",", "m2"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m1", ",", "1", ",", "M"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"m2", ",", "1", ",", "M"}], "}"}]}], "]"}], ",", "1"}], 
    "]"}]}], ";"}]}], "Input",
